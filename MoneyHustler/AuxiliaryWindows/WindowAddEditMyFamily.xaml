<Window x:Class="MoneyHustler.AuxiliaryWindows.WindowAddEditMyFamily"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MoneyHustler.AuxiliaryWindows"
        mc:Ignorable="d"
        Title="WindowAddEditMyFamily" Height="250" Width="500">
    <Grid>
        
        <Grid.RowDefinitions>
            <RowDefinition Height="80*"/>
            <RowDefinition Height="40*"/>
        </Grid.RowDefinitions>

        <TextBox x:Name="TextBoxChangesInput" Grid.Row="0" Width="200" Height="20" IsReadOnly="False"  Margin="10" TextAlignment="Center" />
        <Button Content="Сохранить"  Grid.Row="1" Width="100" Height="30" Click="ButtonSaveClick"  />
            

    </Grid>
</Window>
