<TabItem x:Class="MoneyHustler.Tabs.TabItemStartTab"
            xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
            xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
            Header="Стартовое окно" Background="#FFC3B9A0" BorderBrush="{x:Null}"
            >
    <Grid Margin="0,0,0,0">
        <Image x:Name="MainImage" Grid.Column="3" Grid.Row="0" Source="../Img/Bg.jpg" Stretch="UniformToFill" Grid.RowSpan="11" Panel.ZIndex="0"/>
        <Grid Grid.Column="3" Margin="0,0,167,0" OpacityMask="Black" Background="#BF202020" Grid.RowSpan="11" />
    </Grid>
</TabItem>
